# LangGraph Studio Configuration
# This file configures LangGraph Studio for the WoW RAG API workflow

# Server configuration
LANGGRAPH_STUDIO_PORT=8123
LANGGRAPH_STUDIO_HOST=localhost

# Project configuration
LANGGRAPH_PROJECT_NAME=wow-rag-workflow
LANGGRAPH_PROJECT_DESCRIPTION="Multi-agent workflow for World of Warcraft RAG API"

# API configuration
LANGGRAPH_API_KEY=${LANGGRAPH_API_KEY}
LANGGRAPH_ENDPOINT=${LANGGRAPH_ENDPOINT:-"https://api.langgraph.com"}

# Workflow configuration
WORKFLOW_FILE=src/rag/langgraph/workflow.ts
WORKFLOW_CLASS=LangGraphWorkflow

# Debugging configuration
DEBUG_MODE=true
VERBOSE_LOGGING=true

# Environment variables for the workflow
HUGGINGFACE_API_KEY=${HUGGINGFACE_API_KEY}
EMBEDDINGS_PROVIDER=${EMBEDDINGS_PROVIDER:-"local"}
EMBEDDINGS_API_URL=${EMBEDDINGS_API_URL:-"http://localhost:8000"}
BLIZZARD_CLIENT_ID=${BLIZZARD_CLIENT_ID}
BLIZZARD_CLIENT_SECRET=${BLIZZARD_CLIENT_SECRET}


